{
  "manifest_version": 2,

  "name": "Search on wiki",
  "description": "This extension give a quick way to search on wikiwand.",
  "version": "0.0",
  "icons": {
    "128": "icon-large.png",
    "16": "icon-bitty.png",
    "48": "icon-small.png"
   },
  "browser_action": {
    "default_icon": "icon-large.png",
    "default_popup": "popup.html"
  },
  "author": "Xdminsy",
  "permissions": [
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/"
  ]
}